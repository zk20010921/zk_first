/* 水平间距 */
.iconfont {
  width: 28rpx;
  height: 28rpx;
  margin-right: 10rpx;
  color: #999999;
  font-size: 150rpx;
}
page {
  background: #f6f6f6;
}
uni-radio-group {
  display: block;
  width: 750rpx;
}
.address-list .addr-list .list-con .tops .moren-defalut.hide {
  display: none;
}
.address-list {
  width: 750rpx;
  padding: 0 32rpx 32rpx;
  overflow: hidden;
  min-width: 100%;
  display: block;
}
.address-list .addr-list {
  background: #fff;
  width: 690rpx;
  box-shadow: 0px 10px 28px rgba(0, 0, 0, 0.08);
  display: block;
  border-radius: 16rpx;
  overflow: hidden;
  box-sizing: border-box;
  margin-top: 40rpx;
}
.address-list .addr-list .list-con {
  border-bottom: 1px solid #f5f5f7;
  padding: 22rpx;
  position: relative;
  display: block;
  overflow: hidden;
  width: 690rpx;
}
.address-list .addr-list .list-con .arrow {
  position: absolute;
  height: 80rpx;
  width: 80rpx;
  text-align: center;
  line-height: 80rpx;
  right: 0;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
}
.address-list .addr-list .list-con .tops {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0 0 20rpx;
  font-size: 28rpx;
  color: #333;
}
.address-list .addr-list .list-con .tops .phone {
  margin-left: 20rpx;
}
.address-list .addr-list .list-con .tops .moren-defalut {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 80rpx;
  height: 40rpx;
  margin-left: 40rpx;
  border-radius: 30rpx;
  font-size: 26rpx;
}
.address-list .addr-list .list-con .address {
  padding: 0rpx 60rpx 0 0;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #999;
}
.address-list .addr-list .list-btm {
  padding: 20rpx;
  font-size: 28rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.address-list .addr-list .list-btm .moren {
  color: #333333;
}
.address-list .addr-list .list-btm .caozuo {
  color: #333;
  display: inline-flex;
  align-items: center;
}
.address-list .addr-list .list-btm .caozuo .iconImg {
  width: 32rpx;
  height: 32rpx;
}
.address-list .addr-list .list-btm .caozuo .bianji {
  display: inline-block;
  margin: 0 20rpx 0 10rpx;
}
.address-list .addr-list .list-btm .caozuo .shanchu {
  margin-left: 10rpx;
}
.address-list uni-radio .uni-radio-input.uni-radio-input-checked:before {
  content: '\EA08';
}
.address-list .add.act {
  position: fixed;
  width: 600rpx;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.address-list .add {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 80rpx;
  margin-top: 40rpx;
  text-align: center;
  font-size: 28rpx;
  background-color: #fff;
  border-radius: 16rpx;
}
.address-list .add .iconfont {
  margin-right: 10rpx;
}

