/* 水平间距 */
.list-cell {
  display: flex;
  align-items: baseline;
  line-height: 60rpx;
  position: relative;
  background: #fff;
  justify-content: center;
}
.list-cell.log-out-btn {
  margin-top: 40rpx;
}
.list-cell.log-out-btn .cell-tit {
  text-align: center;
  margin-right: 0;
}
.list-cell.cell-hover {
  background: #fafafa;
}
.list-cell.b-b:after {
  left: 30rpx;
}
.list-cell.m-t {
  margin-top: 16rpx;
}
.list-cell .cell-more {
  align-self: baseline;
  margin-left: 10rpx;
}
.list-cell .cell-tit {
  flex: 1;
  margin-right: 10rpx;
}
.list-cell switch {
  -webkit-transform: translateX(16rpx) scale(0.84);
          transform: translateX(16rpx) scale(0.84);
}

